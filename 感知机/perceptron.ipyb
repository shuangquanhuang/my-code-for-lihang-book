   "source": [
    "# 感知机\n",
    "\n",
    "相当于一个只有一个神经元的神经网络"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 120,
   "metadata": {},
   "outputs": [
    {
     "data": {
